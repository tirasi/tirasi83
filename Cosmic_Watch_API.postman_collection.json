                  "});",
                  "",
                  "pm.test(\"Asteroid removed from watchlist\", function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData).to.have.property('message');",
                  "});"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "DELETE",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/neos/3542519/watch",
              "host": ["{{base_url}}"],
              "path": ["neos", "3542519", "watch"]
            }
          },
          "response": []
        }
      ]
    },
